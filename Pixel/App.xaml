<Application x:Class="Pixel.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:Pixel.Views.Converters"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Themes/Default.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <converters:HotKeyToStringConverter x:Key="HotKeyToStringConverter" />
            <converters:StringToUploaderConverter x:Key="StringToUploaderConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>